<div class="CNS-header">
    <div class="CNS-logo-hamburger-icons-div">
        <div class="CNS-sidebar-icon-div">
            <img id="sidebar-open-close" class="CNS-sidebar-icon" src="/images/menu-white.png">
        </div>
        <div class="CNS-logo-div">
            <i class="CNS-logo-icon fab fa-korvue"></i>
            <div class="CNS-logo-text-div">
                <div class="CNS-logo-text1">Krypto</div>
                <div class="CNS-logo-text2">GYM</div>
            </div>
        </div>
    </div>
    <div class="CNS-header-filler"></div>
    <div class="CNS-account-settings">
        <div id="notification-btn" class="CNS-notification-icon-div">
            <div id="notifications-icon-number-div" class="CNS-notifications-number-div">
                <div id="notifications-icon-number" class="CNS-notifications-number"></div>
            </div>
            <i id="notifications-icon-bell" class="CNS-notification-icon fas fa-bell"></i>
        </div>
        <div id="acc-settings-btn-div" class="CNS-account-settings-icon-div">
            <img id="acc-settings-btn-icon" class="CNS-account-settings-icon" src="images/default-pp.png"/>
        </div>
        <div id="account-dropdown-div" class="CNS-dropdown-div">
            <div id="profile-dd-content" class="CNS-profile-dropdown-content">
                <div class="CNS-profile-custom-top-border"></div>
                <div class="CNS-display-account-info">
                    <div class="CNS-profile-dropdown-image-div">
                        <img id="profile-pic-in-dropdown" class="CNS-profile-dropdown-image" src="images/default-pp.png"/>
                    </div>
                    <div class="CNS-name-account-div">
                        <div id="name" class="CNS-user-name-display"></div>
                        <div id="email" class="CNS-account-name-display"></div>
                    </div>
                </div>
                <div class="CNS-account-features">
                    <img id="certificate"  class="CNS-jop-certificate"/>
                </div>
                <div class="CNS-dropdown-footer">
                    <div id="edit" class="CNS-edit-profile-btn">
                        <div class="CNS-edit-profile-btn-text">Edit Profile</div>
                        <i class="CNS-edit-profile-btn-icon fas fa-user-edit"></i>
                    </div>
                </div>
            </div>
            <div id="notification-dd-content-div" class="CNS-notification-dropdown-content-div">
                <div class="CNS-notifictions-custom-top-border">
                    <div class="CNS-notifictions-custom-top-border1"></div>
                    <div class="CNS-notifictions-custom-top-border-filler"></div>
                    <div class="CNS-notifictions-custom-top-border2"></div>
                </div>
                <div id="notification-dd-content" class="CNS-notification-dropdown-content">
                </div>
            </div>
        </div>
    </div>
</div>
<div id="Sidenav" class="CNS-sidebar">
    <div class="CNS-sidenav-links">
        <a href="/coachHome" class="CNS-sidenav-link">
            <i class="CNS-sidenav-link-icon fas fa-home"></i>
            <div class="CNS-sidenav-link-text">Home</div>
        </a>
        <a href="/coachClients" class="CNS-sidenav-link">
            <i class="CNS-sidenav-link-icon fas fa-users"></i>
            <div class="CNS-sidenav-link-text">Clients</div>
        </a>
        <a href="/coachClientListForChat" id="chat" class="CNS-sidenav-link">
            <i class="CNS-sidenav-link-icon fas fa-comments"></i>
            <div class="CNS-sidenav-link-text">Chat</div>
        </a>
    </div>
    <div id="logout" class="CNS-sidenav-logout-link">
        <i class="CNS-sidenav-link-icon fas fa-sign-out-alt"></i>
        <div class="CNS-sidenav-link-text">LOG OUT</div>
    </div>
</div>
